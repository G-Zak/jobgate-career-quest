<svg width="800" height="400" viewBox="0 0 800 400" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#e2f0ff;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f0f9ff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="400" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="400" y="60" font-family="Arial, sans-serif" font-size="32" font-weight="bold" text-anchor="middle" fill="#2563eb">Numerical Reasoning Test</text>
  <text x="400" y="100" font-family="Arial, sans-serif" font-size="18" text-anchor="middle" fill="#64748b">Assess your ability to interpret and analyze numerical data</text>
  
  <!-- Decorative Elements -->
  <g transform="translate(125, 160)">
    <!-- Bar chart icon -->
    <rect x="0" y="40" width="30" height="60" fill="#3b82f6" rx="2" />
    <rect x="40" y="20" width="30" height="80" fill="#2563eb" rx="2" />
    <rect x="80" y="50" width="30" height="50" fill="#1d4ed8" rx="2" />
    <rect x="120" y="10" width="30" height="90" fill="#3b82f6" rx="2" />
  </g>
  
  <g transform="translate(325, 160)">
    <!-- Pie chart icon -->
    <circle cx="75" cy="75" r="70" fill="none" stroke="#64748b" stroke-width="2" />
    <path d="M75,75 L75,5 A70,70 0 0,1 137,100 z" fill="#3b82f6" />
    <path d="M75,75 L137,100 A70,70 0 0,1 50,140 z" fill="#2563eb" />
    <path d="M75,75 L50,140 A70,70 0 0,1 10,60 z" fill="#1d4ed8" />
    <path d="M75,75 L10,60 A70,70 0 0,1 75,5 z" fill="#60a5fa" />
  </g>
  
  <g transform="translate(550, 160)">
    <!-- Calculator icon -->
    <rect x="10" y="10" width="140" height="140" rx="10" fill="#f8fafc" stroke="#64748b" stroke-width="2" />
    <rect x="30" y="30" width="100" height="30" rx="3" fill="#e2e8f0" />
    <circle cx="45" cy="85" r="10" fill="#cbd5e1" />
    <circle cx="85" cy="85" r="10" fill="#cbd5e1" />
    <circle cx="125" cy="85" r="10" fill="#3b82f6" />
    <circle cx="45" cy="120" r="10" fill="#cbd5e1" />
    <circle cx="85" cy="120" r="10" fill="#cbd5e1" />
    <circle cx="125" cy="120" r="10" fill="#3b82f6" />
  </g>
  
  <!-- Footer Text -->
  <text x="400" y="340" font-family="Arial, sans-serif" font-size="22" font-weight="bold" text-anchor="middle" fill="#334155">Test your numerical skills and data interpretation abilities</text>
  <text x="400" y="370" font-family="Arial, sans-serif" font-size="16" text-anchor="middle" fill="#64748b">Complete within the time limit for the best results</text>
</svg>
