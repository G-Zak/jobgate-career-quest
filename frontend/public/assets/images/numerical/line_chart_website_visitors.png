<svg width="800" height="500" viewBox="0 0 800 500" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bg-gradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#f8fafc;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#f0f9ff;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="500" fill="url(#bg-gradient)" />
  
  <!-- Title -->
  <text x="400" y="50" font-family="Arial, sans-serif" font-size="24" font-weight="bold" text-anchor="middle" fill="#1e3a8a">Website Visitors</text>
  
  <!-- X and Y Axis -->
  <line x1="100" y1="400" x2="700" y2="400" stroke="#64748b" stroke-width="2" />
  <line x1="100" y1="400" x2="100" y2="100" stroke="#64748b" stroke-width="2" />
  
  <!-- X Axis Labels -->
  <text x="100" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Jan</text>
  <text x="200" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Feb</text>
  <text x="300" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Mar</text>
  <text x="400" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Apr</text>
  <text x="500" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">May</text>
  <text x="600" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Jun</text>
  <text x="700" y="425" font-family="Arial, sans-serif" font-size="14" text-anchor="middle" fill="#334155">Jul</text>
  
  <!-- Y Axis Labels -->
  <text x="85" y="400" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">0</text>
  <text x="85" y="340" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">20K</text>
  <text x="85" y="280" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">40K</text>
  <text x="85" y="220" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">60K</text>
  <text x="85" y="160" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">80K</text>
  <text x="85" y="100" font-family="Arial, sans-serif" font-size="14" text-anchor="end" fill="#334155">100K</text>
  
  <!-- Grid Lines -->
  <line x1="100" y1="340" x2="700" y2="340" stroke="#cbd5e1" stroke-width="1" stroke-dasharray="5,5" />
  <line x1="100" y1="280" x2="700" y2="280" stroke="#cbd5e1" stroke-width="1" stroke-dasharray="5,5" />
  <line x1="100" y1="220" x2="700" y2="220" stroke="#cbd5e1" stroke-width="1" stroke-dasharray="5,5" />
  <line x1="100" y1="160" x2="700" y2="160" stroke="#cbd5e1" stroke-width="1" stroke-dasharray="5,5" />
  <line x1="100" y1="100" x2="700" y2="100" stroke="#cbd5e1" stroke-width="1" stroke-dasharray="5,5" />
  
  <!-- Data Line -->
  <polyline points="100,280 200,260 300,220 400,180 500,140 600,120 700,100" 
            fill="none" stroke="#2563eb" stroke-width="3" />
  
  <!-- Data Points -->
  <circle cx="100" cy="280" r="6" fill="#3b82f6" />
  <circle cx="200" cy="260" r="6" fill="#3b82f6" />
  <circle cx="300" cy="220" r="6" fill="#3b82f6" />
  <circle cx="400" cy="180" r="6" fill="#3b82f6" />
  <circle cx="500" cy="140" r="6" fill="#3b82f6" />
  <circle cx="600" cy="120" r="6" fill="#3b82f6" />
  <circle cx="700" cy="100" r="6" fill="#3b82f6" />
  
  <!-- Data Labels -->
  <text x="100" y="265" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">40K</text>
  <text x="200" y="245" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">45K</text>
  <text x="300" y="205" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">60K</text>
  <text x="400" y="165" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">70K</text>
  <text x="500" y="125" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">82K</text>
  <text x="600" y="105" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">88K</text>
  <text x="700" y="85" font-family="Arial, sans-serif" font-size="12" text-anchor="middle" fill="#334155">95K</text>
  
  <!-- Legend -->
  <rect x="600" y="50" width="15" height="3" fill="#2563eb" />
  <text x="625" y="55" font-family="Arial, sans-serif" font-size="14" fill="#334155">Monthly Visitors</text>
</svg>
